<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.matridx.igams.sample.dao.post.IYbsqssdwDao" >
	<!-- 根据申请ID获取机构信息 -->
	<select id="getDto" parameterType="YbsqssdwDto" resultType="YbsqssdwDto">
		select ybdw.sqid,ybdw.xh,ybdw.jgid
			from igams_ybsqssdw ybdw
		<where>
			<if test="sqid != null and sqid != ''">
				and ybdw.sqid = #{sqid}
			</if>
		</where>
	</select>
	<insert id="insert" parameterType="YbsqssdwDto">
		insert into igams_ybsqssdw(sqid,xh,jgid)
			values
		(#{sqid},cast(#{xh} as numeric),#{jgid})
	</insert>
	<update id="delete" parameterType="YbsqssdwDto">
		delete from igams_ybsqssdw
        where sqid =#{sqid}
	</update>
</mapper>
