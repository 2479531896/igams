                <!--条形码方向-->
                <QrBarComboxRotate>2</QrBarComboxRotate>
                <!--条形码类型-->
                <QrBarCodeType>1A</QrBarCodeType>
                <!--条形码中窄单元的宽度-->
                <QrBarCodeNarrowWidth>4</QrBarCodeNarrowWidth>
                <!--条形码中宽单元的宽度-->
                <QrBarCodePHorizontal>8</QrBarCodePHorizontal>
                <!--条码Ascll类型-->
                <!--选’N’ 对应 ASCII 值 78 则不打印条码下面的人可识别文字, -->
                <!--选’B’ 对应 ASCII 值 66 则打印条码下面的人可识别文字.-->
                <QrBarCodePtext>N</QrBarCodePtext>
            </QrContentParamBar>
        </QrContentParamBars>
