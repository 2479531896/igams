    <DataProject Name="仪器码">
        <DataNumber>PRINT_MACHINE</DataNumber>
        <DataProjectName>仪器码</DataProjectName>
        <CodePrintColumn>2</CodePrintColumn>
        <!--打印纸宽度  整体宽度 74mm 600dpi的一个像素0.042mm -->
        <LabelWidthEdit Name="打印纸宽度">1762</LabelWidthEdit>
        <!--打印纸高度  纸张高度 20mm 600dpi的一个像素0.042mm -->
        <LabelHeightEdit Name="打印纸高度">476</LabelHeightEdit>
        <!--标签X轴的坐标偏移值（针对第二）-->
        <LabelWidthOffsets>
            <LabelWidthOffset>
                <WidthOffset Name="G6000">850</WidthOffset>
            </LabelWidthOffset>
            <LabelWidthOffset>
                <WidthOffset Name="C300">850</WidthOffset>
            </LabelWidthOffset>
        </LabelWidthOffsets>
        <LabelContentParams>
            <!--显示在条码下方-->
            <LabelContentParam Name="仪器码_条码号">
                <Number>tmh</Number>
                <Type>字体</Type>
                <!--名称-->
                <Name>仪器码_条码号</Name>
                <!--字体X轴  文字从整个宽度的右边开始算0，越到左边越大，但打印从左到右-->
                <!--新条码：800；旧条码：750-->
                <FontXPosEdit>850</FontXPosEdit>
                <!--字体Y轴 文字从标签的底部开始算0，越到上面越大，但打印从下到上，当前考虑间隔-->
                <!--新条码：60；旧条码：80-->
                <FontYPosEdit>60</FontYPosEdit>
                <!--字体的偏移量-->
                <FontOffset>0</FontOffset>
                <!--字体类型-->
                <FontTypeCombox>宋体</FontTypeCombox>
                <!--字体宽度-->
                <FontWidth>0</FontWidth>
                <!--字体高度-->
                <FontHeight>60</FontHeight>
                <!--字体旋转方向-->
                <FontComboBoxRotate>3</FontComboBoxRotate>
            </LabelContentParam>
            <LabelContentParam Name="仪器码_内部编号">
                <Number>nbbh</Number>
                <Type>字体</Type>
                <!--名称-->
                <Name>仪器码_内部编号</Name>
                <!--字体X轴  文字从整个宽度的右边开始算0，越到左边越大，但打印从左到右-->
                <!--新条码：800；旧条码：750-->
                <FontXPosEdit>850</FontXPosEdit>
                <!--字体Y轴 文字从标签的底部开始算0，越到上面越大，但打印从下到上，当前考虑间隔-->
                <!--新条码：420；旧条码：440-->
                <FontYPosEdit>420</FontYPosEdit>
                <!--字体的偏移量-->
                <FontOffset>0</FontOffset>
                <!--字体类型-->
                <FontTypeCombox>宋体</FontTypeCombox>
                <!--字体宽度-->
                <FontWidth>0</FontWidth>
                <!--字体高度-->
                <FontHeight>60</FontHeight>
                <!--字体旋转方向-->
                <FontComboBoxRotate>3</FontComboBoxRotate>
            </LabelContentParam>
            <LabelContentParam Name="仪器码_文库码">
                <Number>wkh</Number>
                <Type>字体</Type>
                <!--名称-->
                <Name>仪器码_文库码</Name>
                <!--字体X轴-->
                <!--新条码：800；旧条码：750-->
                <FontXPosEdit>850</FontXPosEdit>
                <!--字体Y轴-->
                <!--新条码：360；旧条码：380-->
                <FontYPosEdit>360</FontYPosEdit>
                <!--字体的偏移量-->
                <FontOffset>0</FontOffset>
                <!--字体类型-->
                <FontTypeCombox>宋体</FontTypeCombox>
                <!--字体宽度-->
                <FontWidth>0</FontWidth>
                <!--字体高度-->
                <FontHeight>60</FontHeight>
                <!--字体旋转方向-->
                <FontComboBoxRotate>3</FontComboBoxRotate>
            </LabelContentParam>
        </LabelContentParams>
        <QrContentParamBars>
            <QrContentParamBar Name="仪器码_条形码">
                <Type>条形码</Type>
                <!--编码-->
                <Number>nbbh</Number>
                <!--名称-->
                <Name>仪器码_条形码</Name>
                <!--条形码X轴-->
                <!--新条码：800；旧条码：750-->
                <QrBarCodeXPosEdit>850</QrBarCodeXPosEdit>
                <!--条形码Y轴-->
                <!--新条码：285；旧条码：305-->
