<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.matridx.igams.storehouse.dao.post.IQtckglDao" >
	<insert id="insertQtckgls" parameterType="List">
		INSERT INTO "public"."igams_qtckgl"("qtckid", "ckdh", "wlbm", "sl", "scph", "ckid", "kw", "glid", "glmxid", "lrsj") VALUES
		<if test="list !=null and list.size > 0">
			<foreach collection="list" separator="," item="item" index="index">
				(
				 #{item.qtckid},#{item.ckdh},#{item.wlbm},#{item.sl},#{item.scph},#{item.ckid},#{item.kw} ,#{item.glid},#{item.glmxid},LOCALTIMESTAMP
				)
			</foreach>
		</if>
	</insert>
</mapper>
