<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.matridx.igams.production.dao.matridxsql.Tc_tmp_duplicationDao" >
	<insert id="insert" parameterType="Tc_tmp_duplicationDto" >
		insert into tc_tmp_duplication(
		obj_type
		<if test="code != null and code != ''">
			,code
		</if>
		, oper
		)values(
			1
		<if test="code != null and code != ''">
			,#{code}
		</if>
		,'add'
		)
	</insert>

	<!--修改-->
	<update id="update" parameterType="Sa_invoicecustomersDto">
		update tc_tmp_duplication set
			code=#{code}
		<where>
			code=#{xgqkhdm}
		</where>
	</update>
</mapper>