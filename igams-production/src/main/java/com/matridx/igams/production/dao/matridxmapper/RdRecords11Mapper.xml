	<insert id="insertList" parameterType="List">
		insert into RdRecords11(AutoID,ID,cInvCode,iQuantity,iUnitCost,iPrice,cBatch,dVDate,cPosition,cItem_class,cItemCode,cName,cItemCName,iNQuantity,dMadeDate,
		iMassDate,cMassUnit,iMaIDs,irowno,cbsysbarcode,
