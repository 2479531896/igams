<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.matridx.las.netty.dao.post.IWkhhyypDao" >
<insert id="batchInsert" parameterType="WkhhyypModel">
	insert into igams_wkhhyyp(
	hhyid,
	wkid,
	ybbh,
	x,
	y,
	ispcr,
	sbsj
)
VALUES
<foreach collection="list" separator="," open=""
				close="" item="item" index="index">
(	#{item.hhyid},
	#{item.wkid},
	#{item.ybbh},
	#{item.x},
	#{item.y},
	#{item.ispcr},
	cast(#{item.sbsj} as TIMESTAMP)
	)
	</foreach>
	</insert>
<select id="getDtoList" parameterType="WkhhyypDto" resultType="WkhhyypDto">
select * from igams_wkhhyyp where wkid=#{wkid}
</select>
</mapper>
