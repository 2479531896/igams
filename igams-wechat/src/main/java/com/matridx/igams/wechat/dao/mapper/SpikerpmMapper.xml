<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.matridx.igams.wechat.dao.post.ISpikerpmDao" >
	<select id="getDtoByJcxmAndYblx" parameterType="SpikerpmDto" resultType="SpikerpmDto">
		select yzid,yblx,jcxm,yz
		from igams_spikerpm
		<where>
			scbj!='1' and jcxm=#{jcxm} and yblx=#{yblx}
		</where>
	</select>
</mapper>
