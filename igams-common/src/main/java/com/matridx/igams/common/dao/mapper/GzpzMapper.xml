<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.matridx.igams.common.dao.post.IGzpzDao" >
	<!-- 获取工作配置清单 -->
	<select id="getModelList" parameterType="GzpzDto" resultType="GzpzDto">
		select gzlx,ymlj,lxmc,hdl,hdmx
		from igams_gzpz
	</select>
	
	<!-- 获取工作配置清单 -->
	<select id="getModelById" parameterType="String" resultType="GzpzDto">
		select gzlx,ymlj,lxmc,hdl,hdmx
		from igams_gzpz
		<where>
			gzlx = #{gzlx}
		</where>
	</select>
</mapper>
