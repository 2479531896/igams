<?xml version="1.0" encoding="UTF-8"?>
<!-- 工作配置 -->
<config-task>
	<!-- 
	说明：id区分导入什么；name；class-entity入库对象全限定名；
	class-dao入库DAO spring的ID；page-url 工作提交时显示的页面
	 -->
	<import id="TASK_TYPE" name="研发主任务"
			class-entity="com.matridx.igams.experiment.dao.entities.XmjdrwDto"
			class-service="xmjdrwServiceImpl" 
			page-url="/experiment/project/modWorkTask"/>
	<import id="SUBTASK_TYPE" name="研发子任务"
			class-entity="com.matridx.igams.experiment.dao.entities.XmjdrwDto"
			class-service="xmjdrwServiceImpl" 
			page-url="/experiment/project/modWorkSubtask"/>
</config-task>
