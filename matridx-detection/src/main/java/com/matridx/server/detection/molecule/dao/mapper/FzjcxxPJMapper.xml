    <select id="getListWithMap" parameterType="Map" resultType="FzjcxxDto">
        SELECT
        fzjcxx.fzjcid,
        fzjcxx.ybbh,
        fzjcxx.jcxmmc,
        fzjcxx.syh,
        fzjcxx.xm,
        fzjcxx.xb,
        fzjcxx.nl,
        fzjcxx.qtks,
        fzjcxx.sjdwmc dwmc,
        fzjcxx.zt,
        COALESCE(fzjcxx.yblxmc,yblx_jcsj.csmc) as yblxmc,
        fzjcxx.sfjs,
        to_char( fzjcxx.jssj, 'yyyy-MM-dd HH24:mi' ) jssj,
        fzjcxx.sjdwmc,yyxx.dwjc sjdwjc,yyxx.cskz1 sjdwbj,
        to_char( fzjcxx.sysj, 'yyyy-MM-dd HH24:mi' ) sysj,
        fzjcxx.jcjgmc,
        to_char( fzjcxx.bgrq, 'yyyy-MM-dd HH24:mi' ) bgrq,
        jcdw.csmc jcdwmc,fzjcxx.bbztmc
        FROM
        igams_fzjcxx fzjcxx
        left outer join matridx_jcsj yblx_jcsj on fzjcxx.yblx = yblx_jcsj.csid
        left outer join matridx_jcsj jcdw on fzjcxx.jcdw = jcdw.csid
        left join igams_yyxx yyxx on yyxx.dwid=fzjcxx.sjdw
        <where>
            fzjcxx.scbj != '1'
            and fzjcxx.lrry in
            <foreach collection="lrrys" separator="," open="(" close=")" item="item" index="index">
                #{item}
            </foreach>
            <if test="hzxm != null and hzxm != ''">
                and fzjcxx.xm like '%'||#{xm}||'%'
            </if>
            <if test="ybbh != null and ybbh != ''">
                and fzjcxx.ybbh = #{ybbh}
            </if>
            <if test="sjys != null and sjys != ''">
                and fzjcxx.ysdh = #{sjys}
            </if>
            <if test="entire != null and entire != ''">
                and (
                fzjcxx.xm like '%'||#{entire}||'%'
                or fzjcxx.ybbh like '%'||#{entire}||'%'
                or fzjcxx.sjdwmc like '%'||#{entire}||'%'
                or yyxx.dwjc like '%'||#{entire}||'%'
                or fzjcxx.sjys like '%'||#{entire}||'%'
                )
            </if>
            order by ${sortName} ${sortOrder} LIMIT #{pageSize} OFFSET #{pageStart}
        </where>
    </select>

    <select id="getPjDto" parameterType="FzjcxxDto" resultType="FzjcxxDto">
        select fzjcxx.fzjcid,fzjcxx.ybbh,fzjcxx.bbzbh,fzjcxx.nbbh,fzjcxx.wybh,fzjcxx.jcxmmc,fzjcxx.jcxmid,fzjcxx.syh,fzjcxx.xm,fzjcxx.nl,fzjcxx.lxdh,fzjcxx.sjdwmc,fzjcxx.bgwcsj, fzjcxx.zt,
        fzjcxx.sjys,fzjcxx.ks,fzjcxx.qtks,fzjcxx.ks ksid,ks.dwmc ksmc,fzjcxx.bbztmc,
        to_char(fzjcxx.cjsj,'yyyy-MM-dd hh24:mi:ss') cjsj,
        to_char(fzjcxx.jssj,'yyyy-MM-dd hh24:mi:ss') jssj,
        to_char(fzjcxx.sysj,'yyyy-MM-dd hh24:mi:ss') sysj,
        to_char(fzjcxx.bgrq,'yyyy-mm-dd') bgrq ,
        fzjcxx.jcdw,jcdw_jcsj.csmc jcdwmc,
        fzjcxx.xb, case fzjcxx.xb WHEN '1' THEN '男' WHEN '2' THEN '女' ELSE '未知' END xbmc,
        fzjcxx.yblx,case when yblx_jcsj.cskz1 = '1' then fzjcxx.yblxmc else yblx_jcsj.csmc end yblxmc,fzjcxx.sjdw,fzjcxx.sfjs,fzjcxx.sfsy,yyxx.dwmc as yymc,
        fzjcxx.bz,fzjcxx.jclx,jclx.csdm jclxdm,fzjcxx.zyh,fzjcxx.ysdh,fzjcxx.ch,
        fzjcxx.jcjgmc,yyxx.cskz1 sjdwbj
        from igams_fzjcxx fzjcxx
        left join igams_yyxx yyxx on yyxx.dwid=fzjcxx.sjdw
        left join matridx_jcsj yblx_jcsj on yblx_jcsj.csid=fzjcxx.yblx
        left join matridx_jcsj jcdw_jcsj on jcdw_jcsj.csid=fzjcxx.jcdw
        left join igams_sjdwxx ks on ks.dwid=fzjcxx.ks
        left join matridx_jcsj jclx on jclx.csid=fzjcxx.jclx
        <where>
            fzjcxx.fzjcid=#{fzjcid}
        </where>
    </select>

