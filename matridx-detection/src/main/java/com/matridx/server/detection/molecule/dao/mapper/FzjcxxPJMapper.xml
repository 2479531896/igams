        igams_fzjcxx jc
        left join igams_yyxx xx on xx.dwid=jc.sjdw
        left join igams_hzxx hz on hz.hzid=jc.hzid and hz.scbj='0'
        <where>
            jc.scbj!='1'
            <if test="fzjcid != null and fzjcid != ''">
